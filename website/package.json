    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.39",
    "prettier": "^2.7.1",
    "supabase": "^1.33.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.17",
